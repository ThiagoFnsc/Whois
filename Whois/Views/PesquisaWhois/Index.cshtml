@model IEnumerable<Whois.Models.PesquisaWhois>

@{
    ViewBag.Title = "Histórico";
}

@if (Request.Params.AllKeys.Contains("error")) {
    <div class="alert alert-danger text-center mt-4" role="alert">@Request.Params["error"]</div>
}

<h2 class="mt-3 text-center">Histórico</h2>
@foreach (var item in Model.Reverse()) {
    @Html.Partial("~/Views/PesquisaWhois/Pesquisa.cshtml", item);
}
@if (Model.Count() == 0) {
    <div class="alert alert-info text-center" role="alert">Nenhuma pesquisa foi feita ainda</div>
}